  <table>
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">NOME</th>
        <th scope="col">EMAIL</th>
        <th scope="col">CELULAR</th>
        <th scope="col">SENHA</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i=0; i<dados.length; i++) { %>
        <tr <%- req.session.username==dados[i].username ? `styles="color: white; font-style: bold;"` : ``%>>
          <td><%= dados[i].id %></td>
          <td><%= dados[i].username %></td>
          <td><%= dados[i].email %></td>
          <td><%= dados[i].celular %></td>
          <td><%= dados[i].password %></td>
        </tr>
        <% }%>


        <tfoot>
          <tr>
            <th colspan="5" style="font-size: 16;"><%= `usuario registrado: ${dados.length}` %></th>
          </tr>
        </tfoot>
    </tbody>
  </table>

